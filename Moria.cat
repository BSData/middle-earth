    <selectionEntry id="45fa-1b56-711e-648a" name="The Watcher in the Water" book="Armies of the Lord of the Rings" page="159" hidden="false" collective="false" type="model">
      <profiles>
        <profile id="4698-1838-7a34-ad45" name="The Watcher in the Water" book="Armies of the Lord of the Rings" page="159" hidden="false" profileTypeId="07d0-bd3a-4a2e-7fc3" profileTypeName="Hero/Independent Hero">
          <profiles/>
          <rules/>
