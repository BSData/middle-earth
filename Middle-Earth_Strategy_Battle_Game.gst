      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="6259-22e9-f801-f383" type="max"/>
      </constraints>
      <infoGroups>
        <infoGroup id="1e9c-9429-d48a-1bc3" name="Special Rules" hidden="false">
          <rules>
            <rule id="a12a-adb2-32a5-32d7" name="Death to the Forgoil!" publicationId="b222-d397-7636-8248" page="97" hidden="false">
              <description>Friendly Dunlending models gain the Hatred (Rohan) special rule.</description>
            </rule>
            <rule id="0c93-807a-e15a-fd59" name="Dunlending Pride" publicationId="b222-d397-7636-8248" page="97" hidden="false">
              <description>The range of banners in this army is increased to 6&quot;.</description>
            </rule>
            <rule id="76cb-db8e-fa31-6548" name="Dunlending War Cry" publicationId="b222-d397-7636-8248" page="97" hidden="false">
              <description>Once per game, at the start of any Fight phase, Thrydan can declare that he is using this ability. Until the end of the turn, friendly Dunlending models within 12&quot; of Thrydan gain a bonus of +1 To Wound when making strikes.</description>
            </rule>
          </rules>
