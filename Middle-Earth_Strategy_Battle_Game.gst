    <selectionEntry id="3b82-72e5-cf0d-4faa" name="Assault upon Helm&apos;s Deep" publicationId="b222-d397-7636-8248" page="94" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="3611-a97c-4972-2c7e" type="max"/>
      </constraints>
